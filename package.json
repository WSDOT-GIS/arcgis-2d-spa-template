  },
  "contributors": [
    {
      "name": "Jeff Jacobson",
      "email": "JeffJacobson@users.noreply.github.com"
    }
  ],
  "private": true,
  "license": "Unlicense",
  "devDependencies": {
    "@arcgis/core": "^4.27.6",
    "@esri/arcgis-rest-request": "^4.2.0",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node-lts": "^18.12.2",
    "@types/arcgis-rest-api": "^10.4.5",
    "@types/gh-pages": "^3.2.1",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cspell": "^6.31.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "gh-pages": "^5.0.0",
